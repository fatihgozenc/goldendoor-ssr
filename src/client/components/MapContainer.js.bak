import React from 'react';
import ReactMapGL from 'react-map-gl';

const MapContainer = () => {

  const [config, setConfig] = React.useState({
    viewport: {
      width: 400,
      height: 400,
      latitude: 37.7577,
      longitude: -122.4376,
      zoom: 8
    }
  })

  return (
    <>
      <ReactMapGL
        {...config.viewport}
        onViewportChange={(viewport) => setConfig({viewport})}
      />
    </>
  )
}

export default MapContainer;